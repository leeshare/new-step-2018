/* pages/detail/detail.wxss */

page {
  background: #f5f5f5;
}

.WViewRow1 {
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
  flex-wrap: wrap;
  align-content: stretch;
  width: 750rpx;
  height: 164rpx;
  background-color: #e6e6e6;
  color: #fff;
  font-size: 20PX;
  line-height: 40rpx;
  font-family: "黑体";
}

.winputwrap {
  box-sizing: border-box;
  padding: 0rpx 0rpx 0rpx 10rpx;
  width: 720rpx;
  height: 120rpx;
  background: white;
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
  border-radius: 8rpx;
}

.WViewRow1.focus .winputwrap {
  width: 624rpx;
}

.WViewRow1.focus .WInput17PT1 {
  background-position: left center;
  text-indent: 40rpx;
  width: 100%;
  background-repeat: no-repeat;
}

.WInput17PT1 {
  box-sizing: border-box;
  border: 2rpx #e6e6e6;
  padding: 0rpx 0rpx 0rpx 0;
  width: 100%;
  height: 52rpx;
  background: url(http://www.tohappy.com.cn/web2/skin/images/home/images/search-ico.png) 40% center no-repeat;
  background-size: 40rpx;
  background-color: #fff;
  color: #000;
  font-size: 14PX;
  line-height: 26rpx;
  border-radius: 8rpx;
  font-family: "黑体";
  text-indent: 44%;
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
}

.WViewRow1.focus .winputwrap {
  width: 624rpx;
}

.button-cancel {
  font-size: 14px;
  margin-left: 22rpx;
  color: #666;
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
}

.WInput17PT1.nobj {
  background: none;
  text-indent: 10rpx;
}

.WViewRow1.focus .WInput17PT1 {
  background-position: left center;
  text-indent: 40rpx;
  width: 100%;
  background-repeat: no-repeat;
}

.view_tapwrap {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 13;
}

.placeholder {
  padding-top: 1px;
}

.flow-row-imagebox {
  margin-left: 6rpx;
  margin-top: 6rpx;
  background-color: #f1f1f1;
  float: left;
  overflow: hidden;
}
