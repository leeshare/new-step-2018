/* pages/mycollect/mycollect.wxss */

.dv_img_wrap {
  position: relative;
  margin: 36rpx;
  overflow: hidden;
  border: 2rpx solid #e3e3e3;
}

.dv_img_wrap image {
  height: 100%;
  width: 100%;
}

.title {
  z-index: 1;
  background: white;
  width: 100%;
  left: 0;
  padding: 26rpx 0;
  line-height: 48rpx;
  text-align: center;
  font-size: 18px;
  color: #333;
  font-family: "黑体";
}

.dv_item_img {
  height: 435rpx;
  overflow: hidden;
}

.view_empty_wrap {
  flex-direction: column
}
button[type=warn]{
height:64rpx;
line-height:64rpx;
font-size:14px;
margin-top:35rpx;
width:200rpx;
background:#ea5b5a;
}